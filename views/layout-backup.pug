        title IO Demo
    body
        main.container-fluid(role="main")
            include includes/header.pug
        
            // Begin page content
            div.row
                // Sidebar navigation
                div.sidebar.col-xs-12.col-sm-12.col-md-12.col-lg-2
                        include includes/sidebar.pug
                // Main content
                div.main-content.col-xs-12.col-sm-12.col-md-12.col-lg-10
                    //include includes/sidebar.pug
                    block content
        
        footer
            include includes/footer.pug

        
